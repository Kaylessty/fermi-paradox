<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.StatScreenController">
   <children>
      <VBox layoutX="221.0" layoutY="176.0" prefHeight="200.0" prefWidth="100.0" spacing="5.0">
         <children>
            <Text fill="#eeeb42" strokeType="OUTSIDE" strokeWidth="0.0" text="Name:">
               <font>
                  <Font name="Lao MN" size="13.0" />
               </font></Text>
            <Text fill="#eeeb42" layoutX="10.0" layoutY="23.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Total Money:">
               <font>
                  <Font name="Lao MN" size="13.0" />
               </font></Text>
            <Text fill="#eeeb42" layoutX="10.0" layoutY="40.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Time Spent in Maze:">
               <font>
                  <Font name="Lao MN" size="13.0" />
               </font></Text>
            <Text fill="#eeeb42" layoutX="10.0" layoutY="57.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Monsters Slayed: ">
               <font>
                  <Font name="Lao MN" size="13.0" />
               </font></Text>
            <Text fill="#eeeb42" layoutX="10.0" layoutY="74.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Total Damage Dealt: ">
               <font>
                  <Font name="Lao MN" size="13.0" />
               </font></Text>
         </children></VBox>
      <VBox layoutX="432.0" layoutY="176.0" prefHeight="200.0" prefWidth="100.0">
         <children>
            <Label fx:id="nameLabel" text="Label" textFill="#eeeb42">
               <font>
                  <Font name="Lao MN" size="13.0" />
               </font></Label>
            <Label fx:id="moneyLabel" layoutX="10.0" layoutY="10.0" text="Label" textFill="#eeeb42">
               <font>
                  <Font name="Lao MN" size="13.0" />
               </font></Label>
            <Label fx:id="timeLabel" layoutX="10.0" layoutY="27.0" text="timeLabel" textFill="#eeeb42">
               <font>
                  <Font name="Lao MN" size="13.0" />
               </font></Label>
            <Label fx:id="monstersKilledLabel" layoutX="10.0" layoutY="27.0" text="Label" textFill="#eeeb42">
               <font>
                  <Font name="Lao MN" size="13.0" />
               </font></Label>
            <Label fx:id="damageLabel" layoutX="10.0" layoutY="27.0" text="Label" textFill="#eeeb42">
               <font>
                  <Font name="Lao MN" size="13.0" />
               </font></Label>
         </children></VBox>
      <Button fx:id="exitButton" layoutX="373.0" layoutY="520.0" mnemonicParsing="false" onAction="#exitGame" text="Exit Game" textFill="#e11c1c">
         <font>
            <Font name="Lao MN" size="13.0" />
         </font></Button>
   </children>
</AnchorPane>
