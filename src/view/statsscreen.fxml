<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.StatScreenController">
   <children>
      <VBox layoutX="221.0" layoutY="176.0" prefHeight="200.0" prefWidth="100.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Name:" />
            <Text layoutX="10.0" layoutY="23.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Total Money:" />
            <Text layoutX="10.0" layoutY="40.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Time Spent in Maze:" />
            <Text layoutX="10.0" layoutY="57.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Monsters Slayed: " />
            <Text layoutX="10.0" layoutY="74.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Total Damage Dealt: " />
         </children></VBox>
      <VBox layoutX="432.0" layoutY="176.0" prefHeight="200.0" prefWidth="100.0">
         <children>
            <Label fx:id="nameLabel" text="Label" />
            <Label fx:id="moneyLabel" layoutX="10.0" layoutY="10.0" text="Label" />
            <Label fx:id="timeLabel" layoutX="10.0" layoutY="27.0" text="timeLabel" />
            <Label fx:id="monstersKilledLabel" layoutX="10.0" layoutY="27.0" text="Label" />
            <Label fx:id="damageLabel" layoutX="10.0" layoutY="27.0" text="Label" />
         </children></VBox>
      <Button fx:id="exitButton" layoutX="373.0" layoutY="520.0" mnemonicParsing="false" onAction="#exitGame" text="Exit Game" />
   </children>
</AnchorPane>
